#include <stdio.h>

int main() {
    int nums[] = {1, 2, 3, 4, 5}, n = 5;
    printf("Even numbers: ");
    for (int i = 0; i < n; i++)
        if (nums[i] % 2 == 0) printf("%d ", nums[i]);  
    printf("\nOdd numbers: ");
    for (int i = 0; i < n; i++)
        if (nums[i] % 2 != 0) printf("%d ", nums[i]); 
    return 0;
}
