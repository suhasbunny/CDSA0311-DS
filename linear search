#include <stdio.h>

int linear_search(int arr[], int size, int target) {
    for (int i = 0; i < size; i++) 
        if (arr[i] == target) return i; 
    return -1; 
}

int main() {
    int arr[] = {1, 2, 3, 4, 5}, target = 3;
    printf("Index of %d: %d\n", target, linear_search(arr,Â 5,Â target));
}
